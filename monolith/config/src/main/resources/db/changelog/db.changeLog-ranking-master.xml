<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.22.xsd">

    <property name="context" value="ranking" dbms="postgresql"/>
    <property name="schemaRanking" value="ranking" dbms="postgresql" context="ranking"/>
    <preConditions onFail="WARN">
        <dbms type="postgresql"/>
    </preConditions>
    <includeAll path="db/changelog/ranking-version"/>
</databaseChangeLog>